cluster:
  name: ${cluster_name}

network.host: "0.0.0.0"

path:
  data: /var/lib/elasticsearch
  logs: /var/log/elasticsearch

bootstrap:
  memory_lock: true

http:
  cors:
    enabled: true
    allow-origin: "*"

cloud.gce.project_id: ${project}
cloud.gce.zone: [${zones}]
discovery:
  seed_providers: gce

xpack:
  security:
    enabled: false
    transport:
      ssl:
        enabled: false
    http:
      ssl:
        enabled: false

# config generated by Terraform provision
